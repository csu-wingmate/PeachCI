<StateModel name="dns" initialState="test">
    <State name="test">
        <Action type="output">
            <DataModel ref="p1" />
            <DataModel ref="p2" />
            <DataModel ref="p3" />
        </Action>
    </State>

</StateModel>



<Agent name="LinAgent" >
    <Monitor class="Process" name="dns">
            <Param name="Executable" value="/usr/local/sbin/dnsmasq" />
            <Param name="Arguments" value=" -p 5353 -d -C /opt/dnsmasq-2.90/conf/dnsmasq.conf -8 /var/log/dnsmasq/dnsmasq.log" />
            <Param name="RestartOnEachTest" value="true" />
            <Param name="Faultonearlyexit" value="False" />
            <!-- <Param name="StartOnCall" value="hexdump"/> -->
    </Monitor>
</Agent> 

<Test name="Default" replayEnabled="false">
        <Agent ref="LinAgent" platform="linux"/>
        <StateModel ref="dns"/>
        <Publisher class="Udp" name="multicast">
            <Param name="Host" value="127.0.0.1" />
            <Param name="Port" value="5353" />
        </Publisher>
        <Logger class="File">
          <Param name="Path" value="/root/protocolFuzz/dns/peach/logs"/>
        </Logger>
</Test>

</Peach>

    